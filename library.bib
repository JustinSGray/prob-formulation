@ARTICLE{Soni2010,
  author = {Soni, Devpriya and Shrivastava, Ritu and Kumar, M},
  title = {{A Framework for Validation of Object Oriented Design Metrics}},
  journal = {Proceedings of World Academy of Science Engineering and Technology},
  year = {2010},
  volume = {60},
  pages = {7},
  abstract = {A large number of metrics have been proposed for the quality of object
	oriented software. Many of these metrics have not been properly validated
	due to poor methods of validation and non acceptance of metrics on
	scientific grounds. In the literature, two types of validations namely
