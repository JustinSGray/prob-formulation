@CONFERENCE{Martins2012,
  author = {Joaquim R. R. A. Martins and Hwang, John T},
  title = {Review and Unification of Methods for Computing Derivatives of Multidisciplinary
	Systems},
  booktitle = {In Proceedings of the 53rd AIAA/ASME/ASCE/AHS/ASC Structures, Structural
	Dynamics, and Materials Conference},
  year = {2012},
  address = {Honolulu, HI},
  month = {April},
  note = {AIAA 2012-1589},
  abstract = {<p>This paper presents a comprehensive review of all the options available
	for computing derivatives of multidisciplinary systems in a unified
	mathematical framework. The basic building blocks for computing derivatives
	are first introduced: finite differencing, the complex-step method
	and symbolic differentiation. A generalized chain rule is derived
	from which it is possible to derive both algorithmic differentiation
	and analytic methods. This generalized chain rule is shown to have
	two forms --- a forward form and a reverse form --- which correspond
	to the forward and reverse modes in algorithmic differentiation and
	the direct and adjoint approaches in analytic methods. Finally, the
	theory is extended to methods for computing derivatives of multidisciplinary
	systems, and several new insights are provided.</p>}
}

